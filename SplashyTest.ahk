	switch i
	{
		case 1, 3, 4, 8, 9, 10, 11, 12, 14, 30, 31, 32, 42, 43, 44:
		return 1
		case 7, 22, 28, 34, 40:
		{
		spr := ChooseColor(0x80FF, thisHWnd, , , Colors*)
